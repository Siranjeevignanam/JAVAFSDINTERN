<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
                
                
                 <bean id="ad" class="com.test.Address">
     <property name="city" value="Vadachennai"></property>
     <property name="state" value="Tamilnadu"></property>
     <property name="pincode" value="600028"></property>
     
     
     </bean>
     
     
     <bean id="cus" class="com.test.Customer">
 
     <property name="name" value="Rajan">  </property>
      <property name="country" value="India"></property>
     <property name="orders">
     <list>
     <value>Boat</value>
       <value>Knife</value>
          <value>food</value>
              <value>water bottle</value>        
     </list>
     </property>
    <property name="adr" ref="ad"></property>
     </bean>          
                
                
                </beans>